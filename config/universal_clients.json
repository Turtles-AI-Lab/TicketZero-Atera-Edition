  "industry_templates": {
    "msp": {
      "common_issues": ["email_problems", "vpn_issues", "software_installation", "printer_problems", "network_connectivity"],
      "typical_resolution_time": 15,
      "escalation_rate": 25,
      "automation_rate": 75
    },
    "hospitality": {
      "common_issues": ["wifi_problems", "tv_issues", "room_maintenance", "guest_complaints", "housekeeping_requests"],
      "typical_resolution_time": 20,
      "escalation_rate": 15,
      "automation_rate": 85
    },
    "enterprise": {
      "common_issues": ["software_support", "hardware_issues", "account_access", "training_requests", "system_outages"],
      "typical_resolution_time": 25,
      "escalation_rate": 35,
      "automation_rate": 65
    },
    "healthcare": {
      "common_issues": ["emr_issues", "network_problems", "device_connectivity", "software_support", "security_alerts"],
      "typical_resolution_time": 10,
      "escalation_rate": 45,
      "automation_rate": 55
    },
    "education": {
      "common_issues": ["lms_problems", "email_issues", "wifi_connectivity", "software_licensing", "classroom_tech"],
      "typical_resolution_time": 30,
      "escalation_rate": 30,
      "automation_rate": 70
