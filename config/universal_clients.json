      "endpoint": "${AZURE_OPENAI_ENDPOINT}",
      "api_key": "${AZURE_OPENAI_API_KEY}",
      "deployment": "${AZURE_OPENAI_DEPLOYMENT}",
      "model": "gpt-3.5-turbo",
      "max_tokens": 200,
      "temperature": 0.3
    },
    "openai": {
      "api_key": "${OPENAI_API_KEY}",
      "model": "gpt-3.5-turbo",
      "max_tokens": 200,
      "temperature": 0.3
    },
    "lmstudio": {
      "endpoint": "http://localhost:1234/v1",
      "model": "phi-3-mini-4k-instruct",
      "max_tokens": 150,
      "temperature": 0.3
    }
  },
